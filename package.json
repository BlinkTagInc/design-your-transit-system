{
  "name": "design-your-transit-system",
  "author": "Brendan Nee <brendan@blinktag.com>",
  "version": "0.3.6",
  "dependencies": {
    "@hapi/basic": "^6.0.0",
    "@hapi/hapi": "^19.1.1",
    "@mars/heroku-nextjs-build": "^2.0.0",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "classnames": "^2.2.6",
    "color": "^3.1.2",
    "dotenv": "^8.2.0",
    "es6-promise": "^4.2.8",
    "eslint-config-xo-nextjs": "^1.6.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.1",
    "hapi-require-https": "^5.0.0",
    "isomorphic-fetch": "^2.2.1",
    "json2csv": "^5.0.1",
    "mongoose": "^5.9.13",
    "next": "^9.4.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-sticky": "^6.0.3",
    "styled-jsx-plugin-sass": "^1.0.0"
  },
  "devDependencies": {
    "xo": "^0.30.0"
  },
  "engines": {
    "node": "12.16.3"
  },
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "node server.js"
  },
  "cacheDirectories": [
    "nextjs/node_modules"
  ],
  "license": "MIT",
  "heroku-run-build-script": true,
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^[A-Z]"
        }
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  }
}
