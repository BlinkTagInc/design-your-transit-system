    "xo": "^0.33.1"
  },
  "engines": {
    "node": "12.x"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^[A-Z]"
        }
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  }
