    "eslint-config-xo-nextjs": "^1.6.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "xo": "^0.34.1"
  },
  "engines": {
    "node": "12.x"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^[A-Z]"
        }
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  }
}
