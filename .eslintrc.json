{
  "extends": "next/core-web-vitals",
  "rules": {
    "camelcase": [
      "error",
      {
        "properties": "never"
      }
    ],
    "indent": ["error", 2],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "^[A-Z]"
      }
    ],
    "semi": [
      "error",
      "never"
    ]
  }
}
